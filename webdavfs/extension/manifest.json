{
  "name": "WebDAV File System",
  "description": "Access a WebDAV server directly from the Files app.",
  "version": "0.0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icon/16.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "fileSystemProvider",
    "storage"
  ]
}
